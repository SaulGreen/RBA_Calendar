<script type="text/javascript">
	$(document).on("ready",function(){

		var allDay = false;

		$("#btnAdv").on("click", function(){
			var annType = $("#slctAnnType option:selected").val();
			var annUser = $("#slctUserAnn option:selected").val();
			var authOpt = $("#slctAdminAut option:selected").val();
			var title = $("#txtTitle").val();
			var dtAviso = $("#dtAviso").val();
			var comment = $("#txtCmnt").val();
			var hrOut = $("#slctOtAd option:selected").val();
			var hrIn = $("#slctInAd option:selected").val();

			var data = { titulo: title, fecha: dtAviso, comentario: comment, horainicio: hrOut, horafinal: hrIn, allDay: allDay, user: annUser, auth: authOpt, type: annType }
			console.log(data)
			$.ajax({
				type: "POST",
				url: "/log/SetAnnouncement",
				dataType: "json",
				contentType: "application/json; charset-UTF8",
				data: JSON.stringify(data),
				success: function(resp){
					if(resp != null){
						alert("Aviso generado con exito.")
						console.log(resp);
					}
				},error: function(xhr,status,error){
					console.log(xhr);
					alert(error);
				}
			})
		});

		$("#slctAnnType").on("change", function(){
			var advType = $("#slctAnnType option:selected").val();

			if(advType != 1){
				$(".user-group").hide();
			}else{
				$(".user-group").show();
		}
		})

		$("body").on("change", "#chkAll", function(){
			var elem = $(this);

			if(elem.is(":checked")){
				$("#slctOtAd").prop("selectedIndex",0).prop("disabled","disabled");
				$("#slctInAd").prop("selectedIndex",0).prop("disabled","disabled");
				allDay = true;
			}else{
				$("#slctOtAd").prop("selectedIndex",0).prop("disabled",false);
				$("#slctInAd").prop("selectedIndex",0).prop("disabled",false);
				allDay = false;
			}
		})

		$("#dashInfo").on("click",function(){
			$.ajax({
				url:"/log/colorList",
				type:"get",
				data:'json',
				contentType:"application/json; charset-UTF8",
				success: function(resp){
					$(".tblBdyColor").empty();
					var tr = "";
					$(resp).each(function(i,e){
						tr += "<tr><td style='background-color:" + e.color + "; color:white;'>" + e.color + "</td><td style='background-color:" + e.colorsec + "; color:white;'>" + e.colorsec + "</td><td>" + e.nombre + " " + e.apaterno + "</td><td>" + e.used + "</td></tr>"
					})
					$(".tblBdyColor").append(tr);
				}, error: function(xhr,status,error){
					console.log(xhr);
					alert(error)
				}
			})
		});

		$("#btnUsrs").on("click", function(){
			var status = $("#slctSt option:selected").val();
			var role = $("#slctRl option:selected").val();

			var data = { status: status, role: role}
			$.ajax({
				url:"/log/GetUsersByStatus",
				type:"POST",
				dataType: "json",
				contentType: "application/json; charset-UTF8",
				data: JSON.stringify(data),
				success: function(resp){
					var cnt = 1, tr = "";
					$(".tblDataUsrs").empty();
					$(resp).each(function(i,e) {

						var roles = ["Administrador","Gerente","Asesor","Recepcion"];
						if(e.rol == 1)
							rol = roles[0]
						else if(e.rol == 2)
							rol = roles[1]
						else if(e.rol == 3)
							rol = roles[2]
						else if(e.rol == 4)
							rol = roles[3]

						if(e.status == 2)
							status = "<td><span class='label label-success'>Activo</span></td>";
						else if(e.status == 1)
							status = "<td><span class='label label-danger'>Inactivo</span></td>";
						else 
							status = "<td><span class='label label-warning'>Pendiente</span></td>";


						tr += "<tr data-usr='" + e.id + "'><td>" + cnt + "</td><td>" + e.nombre + " " + e.apaterno + "</td><td>" + e.email + "</td><td>" + e.telefono + "</td><td>" + rol + "</td><td>" + moment(e.fechaIng).format("DD-MM-YYYY") + "</td>" + status + "<td>" + e.ip + "</td><td><a class='btn btn-success btn-xs actvUsr'><span class='fa fa-check fa-1'></span></a> <a class='btn btn-info btn-xs edtUsr'><span class='fa fa-pencil-square-o fa-1'></span></a> <a class='btn btn-warning btn-xs dltUsr'><span class='fa fa-ban fa-1'></span></a> <a class='btn btn-danger btn-xs dctvUsr'><span class='fa fa-times fa-1'></span></a></td></tr>";

						cnt++;
					});

					$(".tblDataUsrs").append(tr);
				}, error: function(xhr, status, error){
					console.log(xhr);
					alert(error)
				}
			})
		})

		$.ajax({
			type: "get",
			url: "/log/GetLogActions",
			dataType: "json",
			contentType: "application/json",
			success: function(resp){
				var opt = "";
				$(resp).each(function(i,e){
					opt += "<option value='" + e.id + "'>" + e.accion + "</option>"
				})
				$("#slctActn").append(opt);
			}, error: function(xhr,status,error){
               	console.log(xhr);
            	alert(error);
            }
		})

		$.ajax({
			type: "get",
			url: "/log/GetUsersLog",
			dataType: "json",
			contentType: "application/json",
			success: function(resp){
				var opt = "";
				$(resp).each(function(i,e){
					opt += "<option value='" + e.id + "'>" + e.nombre + " " + e.apaterno + "</option>"
				})
				$("#slctUsr").append(opt);
				$("#slctUserAnn").append(opt);
				$("#slctUsrEdt").append(opt);
			}, error: function(xhr,status,error){
               	console.log(xhr);
            	alert(error);
            }
		});

		$.ajax({
			url:"/log/colorList",
			type:"get",
			data:'json',
